/**
 * Exclusive-or gate:
 * out = not (a == b)
 */

CHIP Xor {
    IN a, b;
    OUT out;

    PARTS:
    // Put your code here:
    And(a=a,b=b,out=out2);
    Not(in=out2,out=out3);
    Or(a=a,b=b,out=out1);
    And(a=out1,b=out3,out=out);

}